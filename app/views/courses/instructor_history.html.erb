<h1>Listing courses</h1>

<table>
  <tr>
    <th>Course Number</th>
    <th>Title</th>
    <th>Instructor</th>
    <th>Status</th>
    <th colspan="3"></th>
  </tr>
 
  <% @courses.each do |course| %>
    <tr>
      <td><%= course.course_number %></td>
      <td><%= course.title %></td>
      <td><%= course.user.email %></td>
      <td><%= course.status %></td>
      
    </tr>
  <% end %>
</table>
