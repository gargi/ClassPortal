<h1>Listing enrollments</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Course</th>
    <th>Status</th>
    <th>Grade</th>
    <th colspan="2"></th>
  </tr>
 
  <% @enrollments.each do |enrollment| %>
    <tr>
      <td><%= User.find(enrollment.user_id).email %></td>
      <td><%= Course.find(enrollment.course_id).title %></td>
      <td><%= enrollment.status %></td>
      <td><%= enrollment.grade %></td>
      
    </tr>
  <% end %>
</table>

