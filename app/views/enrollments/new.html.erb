<h1>New enrollment</h1>

<%= form_for :enrollment, url: enrollments_path do |f| %>


    <p>
      <%= f.label :user %><br>
      <%= f.text_field :user %>
    </p>

    <p>
      <%= f.label :course %><br>
      <%= f.text_field :course %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>

<%= link_to 'Back', enrollments_path %>
