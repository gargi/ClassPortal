<h1>New Announcement</h1>

<%= form_for :material, url: materials_path do |f| %>


    <p>
      <%= f.label :course %><br>
      <%= f.select :course_id, options_from_collection_for_select(Course.all, :id, :title, 1) %>
    </p>


    <p>
      <%= f.label :content %><br>
      <%= f.text_area :content %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>

<%= link_to 'Back', materials_path %>
